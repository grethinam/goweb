{{ define "Index" }}
 {{ template "Header" }}
	
	<table border="1">
      <tr>
	    <td>Id</td>
        <td>FirstName</td>
        <td>SecondName</td>
        <td>Department</td>
        <td>Email</td>
		<td>Edit</td>
		<td>Delete</td>
      </tr>
    {{ range . }}
      <tr>
		<td>{{ .Id }}</td>
        <td>{{ .Fname }}</td>
        <td> {{ .Sname }} </td>
        <td>{{ .Dname }} </td>
		<td>{{ .Email }} </td>
		<td><a href="/edit?id={{ .Id }}">Edit</a></td>
        <td><a href="/delete?id={{ .Id }}">Delete</a><td>
      </tr>
    {{ end }}
    </table>
  {{ template "Footer" }}
{{ end }}
